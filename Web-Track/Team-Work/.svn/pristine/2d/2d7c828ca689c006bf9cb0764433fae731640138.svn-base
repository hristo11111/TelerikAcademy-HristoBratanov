<fieldset>
    <legend>Login</legend>
    <label for="tb-username">Username</label>
    <input data-bind="value: username" type="text" id="tb-username" />
    <label for="tb-password">Password</label>
    <input data-bind="value: password" type="text" id="tb-password" />
    <button data-bind="click: login">Login</button>
</fieldset>
<fieldset>
    <legend>Register</legend>
    <label for="tb-usernameReg">Username</label>
    <input data-bind="value: usernameReg" type="text" id="tb-usernameReg" />
    <label for="tb-passwordReg">Password</label>
    <input data-bind="value: passwordReg" type="text" id="tb-passwordReg" />
    <label for="tb-firstName">First Name</label>
    <input data-bind="value: firstName" type="text" id="tb-firstName" />
    <label for="tb-lastName">Last Name</label>
    <input data-bind="value: lastName" type="text" id="tb-lastName" />
    <label for="tb-email">Email</label>
    <input data-bind="value: email" type="text" id="tb-email" />
    <button data-bind="click: register">Register</button>
</fieldset>
